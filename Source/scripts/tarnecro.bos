StartBuilding()
{
	set UNKNOWN_UNIT_VALUE(5) to 1;
	building = TRUE;
	
	restored = FALSE;
	while (!restored OR direction)
	{
		sleep 100;
	}
	
	if (building AND !(get UNKNOWN_UNIT_VALUE(29) > 0)) {
		build_anim = TRUE;
		startb_anim = TRUE;
		call-script startbuild();
		startb_anim = FALSE;
		
		while (building AND !building_anim)
		{
			building_anim = TRUE;
			call-script build();
			building_anim = FALSE;
		}
	}
}

StopBuilding()
{
	set UNKNOWN_UNIT_VALUE(5) to 0;
	building = FALSE;
	stopb_anim = TRUE;
	call-script stopbuild();
	stopb_anim = FALSE;
	build_anim = FALSE;
}

MoveWatcher()
{
	set-signal-mask SIG_MOVE;
	while( TRUE )
	{
		moving = (get UNKNOWN_UNIT_VALUE(29)) > 5 OR direction;
		sleep 100;
	}
}

MeleeControl()
{             
	while( TRUE )
	{
		if ((get UNKNOWN_UNIT_VALUE(5) == 1) AND (get UNKNOWN_UNIT_VALUE(29) > 0))
		{
			set UNKNOWN_UNIT_VALUE(5) to 0;
			building = FALSE;
			build_anim = FALSE;
		}
		if( moving )
		{
			if(firing OR build_anim)
			{
				call-script walk_legs();
			}
			else if( get UNKNOWN_UNIT_VALUE(28) )
			{
				call-script walk_water();
			}
			else if( get UNKNOWN_UNIT_VALUE(34) )
			{
				call-script walk_road();
			}
			else
			{
				call-script walk();
			}
			legs_restored = FALSE;
			restored = FALSE;
		}
		else if( !busy_dying )
		{
			if( !legs_restored )
			{
				call-script restore_legs();
				legs_restored = TRUE;
			}
			else if(( !restored ) AND ( !aiming ))
			{
				call-script restore_x();
				restored = TRUE;
			}
			else
			{
				sleep 10;
			}
		}
		else
		{
			sleep 10;
		}
	}
}

Create()
{
	move emitgronk to x-axis [-1.000000] now;
	move emitgronk to y-axis [9.000000] now;
	move emitgronk to z-axis [18.000000] now;
	move emitgronk2 to x-axis [-4.000000] now;
	move emitgronk2 to y-axis [10.000000] now;
	move emitgronk2 to z-axis [21.000000] now;
	moving = FALSE;
	aiming = FALSE;
	firing = FALSE;
	building = FALSE;
	busy_dying = FALSE;
	kill_attack = FALSE;
	legs_restored = FALSE;
	restored = FALSE;
	direction = 0;
	weapon = 0;
	restore_delay = 5000;
	build_anim = FALSE;
	stopb_anim = FALSE;
	startb_anim = FALSE;
	building_anim = FALSE;
	while (get UNKNOWN_UNIT_VALUE(17))
	{
		sleep 100;
	}
	start-script MoveWatcher();
	start-script MeleeControl();
}

TurnDirection(Func_Var_1)
{
	direction = Func_Var_1;
}

SetMaxReloadTime(Func_Var_1)
{
	restore_delay = Func_Var_1 * 2;
}

RestoreAfterDelay()
{
	signal SIG_RESTORE;
	set-signal-mask SIG_RESTORE;
	var Func_Var_1;
	Func_Var_1 = 0;
	while( Func_Var_1 < restore_delay )
	{
		if( kill_attack )
		{
			Func_Var_1 = restore_delay;
		}
		else
		{
			Func_Var_1 = Func_Var_1 + 100;
		}
		sleep 100;
	}
	while( firing )
	{
		sleep 100;
	}
	set UNKNOWN_UNIT_VALUE(21) to weapon;
	aiming = FALSE;
	if( building )
	{
		call-script startbuild();
		restored = FALSE;
	}
}

AimWeapon(Func_Var_1, Func_Var_2, Func_Var_3)
{
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	aiming = TRUE;
	while( firing )
	{
		sleep 100;
	}
	if( building )
	{
		call-script restore_x();
		sleep 250;
	}
	weapon = Func_Var_3;
	start-script RestoreAfterDelay();
	set UNKNOWN_UNIT_VALUE(22) to Func_Var_3;
}

FireWeapon(Func_Var_1)
{
	firing = TRUE;
	kill_attack = FALSE;
	if( Func_Var_1 == 0 )
	{
		call-script attack1(Func_Var_1);
	}
	else if( Func_Var_1 == 1 )
	{
		call-script attack2(Func_Var_1);
	}
	else
	{
		call-script attack3(Func_Var_1);
	}
	firing = FALSE;
}

TargetCleared(Func_Var_1)
{
	set UNKNOWN_UNIT_VALUE(21) to Func_Var_1;
	kill_attack = TRUE;
	signal SIG_AIM;
}

QueryWeapon(Func_Var_1, Func_Var_2)
{
	if( Func_Var_2 == 1 )
	{
		Func_Var_1 = 2;
	}
	else
	{
		Func_Var_1 = 1;
	}
}

SweetSpot(Func_Var_1)
{
	Func_Var_1 = 0;
}

QueryBlood(Func_Var_1)
{
	Func_Var_1 = 3 + Rand( 0, 2 );
}

Dying()
{
	set-signal-mask 0;
	signal SIG_AIM;
	signal SIG_MOVE;
	signal SIG_RESTORE;
	moving = FALSE;
	busy_dying = TRUE;
	set UNKNOWN_UNIT_VALUE(31) to 1;
}

Killed(Func_Var_1, Func_Var_2, Func_Var_3)
{
	Func_Var_2 = 0;
	if( Func_Var_3 == 3 )
	{
		explode staff type FALL;
		explode ruparm type FALL;
		explode luparm type FALL;
		explode head type FALL;
		explode torso type FALL;
		explode rupleg type FALL;
		explode lupleg type FALL;
	}
}
